<Window x:Class="ExcelExport.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExcelExport"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="NPOIButton" Content="NPOI" Click="NPOIButton_Click" HorizontalAlignment="Left" Height="32" Margin="65,38,0,0" VerticalAlignment="Top" Width="91"/>
        <Button x:Name="EPPlusButton" Content="EPPlus" Click="EPPlusButton_Click" HorizontalAlignment="Left" Height="32" Margin="65,104,0,0" VerticalAlignment="Top" Width="91"/>
        <Button x:Name="ClosedXMLButton" Content="ClosedXML" Click="ClosedXMLButton_Click" HorizontalAlignment="Left" Height="32" Margin="65,167,0,0" VerticalAlignment="Top" Width="91"/>
        <Button x:Name="OleDBButton" Content="OleDB" Click="OleDBButton_Click" HorizontalAlignment="Left" Height="32" Margin="65,231,0,0" VerticalAlignment="Top" Width="91"/>
        <StatusBar Name="Status" HorizontalAlignment="Stretch"  Height="30" VerticalAlignment="Bottom">
            <StatusBarItem>
                <TextBlock Name="StatusText"/>
            </StatusBarItem>
        </StatusBar>
    </Grid>
</Window>
